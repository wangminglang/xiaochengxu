/* pages/fourth/center/center.wxss */
.sectionOne{
    width: 100%;
    height: 450rpx;
    background-color: #56d1a8;
}

.headImg{
    width: 100%;
    height: 300rpx;
    position: relative;
}
.headImg .center{
    display: block;
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
    position: absolute;
    left: 300rpx;
    top: 20rpx;
}
.headImg .name{
    color: white;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 32rpx;
    text-align: center;
    width: 150rpx;
    border: 1px solid white;
    border-radius: 10rpx;
    position: absolute;
    left: 300rpx;
    top: 200rpx;
}
.headImg .right{
    display: block;
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    width: 50rpx;
    height: 40rpx;
}

.headList{
    width: 100%;
    height: 150rpx;
    background: white;
}
.headList .headView{
    width: 24%;
    height: 100%;
    display: inline-block;
    border-left: 1px solid #eaeaea;
}
.headList .headView .num{
    font-size: 50rpx;
    color: red;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    display: block;
}
.headList .headView .title{
    width: 100%;
    height: 50rpx;
    text-align: center;
    display: block;
    font-size: 32rpx;
}

.sectionTwo, .sectionThree{
    width: 100%;
    height: 250rpx;
    margin-top: 25rpx;
    background-color: white;
}
.sectionTwo .title, .sectionThree .title{
    width: 100%;
    height: 100rpx;
    border-bottom: 1px solid #eaeaea;
}
.sectionTwo .title image, .sectionThree .title image{
    width: 60rpx;
    height: 60rpx;
    margin-top: 20rpx;
    margin-left: 20rpx;
    float: left;
}
.sectionTwo .title text, .sectionThree .title text{
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 40rpx;
    margin-left: 20rpx;
}
.sectionTwo .twoList, .sectionThree .threeList{
    width: 100%;
    height: 150rpx;
}
.sectionTwo .twoList .twoView{
    width: 33%;
    height: 100%;
    display: inline-block;
    border-left: 1px solid #eaeaea;
}
.sectionThree .threeList .threeView{
    width: 49%;
    height: 100%;
    display: inline-block;
    border-left: 1px solid #eaeaea;
}
.sectionTwo .twoList .twoView .num, .sectionThree .threeList .threeView .num{
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    display: block;
}
.sectionTwo .twoList .twoView .bottom, .sectionThree .threeList .threeView .bottom{
    width: 100%;
    height: 70rpx;
    position: relative;
}
.sectionTwo .twoList .twoView .bottom image{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    top: 10rpx;
    left: 20rpx;
}
.sectionTwo .twoList .twoView .bottom text{
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    position: ablolute;
    margin-left: 80rpx;
}
.sectionThree .threeList .bottom image{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    top: 10rpx;
    left: 100rpx;
}
.sectionThree .threeList .threeView .bottom text{
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    position: ablolute;
    margin-left: 170rpx;
}

.sectionFour{
    width: 100%;
    background-color: white;
    margin-top: 25rpx;
}
.sectionFour .fourView{
    width: 100%;
    height: 100rpx;
    border-bottom: 1px solid #eaeaea;
    position: relative;
}
.sectionFour .fourView .imageLeft{
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    top: 20rpx;
    left: 20rpx;
}
.sectionFour .fourView .imageRight{
    height: 30rpx;
    width: 20rpx;
    top: 45rpx;
    position: absolute;
    right: 20rpx;
}
.sectionFour .fourView view{
    height: 100rpx;
    line-height: 100rpx;
    left: 100rpx;
    position: absolute;
}